{
  "extends": [
    "config:best-practices",
    ":disableRateLimiting"
  ],
  "automergeType": "pr",
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "automerge": true,
      "automergeType": "pr"
    },
    {
      "matchManagers": ["custom.regex"],
      "automerge": false
    }
  ],
  "enabledManagers": [
    "github-actions",
    "custom.regex"
  ],
  "assignees": [
    "pluggero"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "description": "nvim-telescope/telescope.nvim",
      "fileMatch": ["^lua/config/plugins/telescope\\.lua$"],
      "matchStrings": [
        "\"nvim-telescope/telescope\\.nvim\"[^}]*version\\s*=\\s*\"(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "nvim-telescope/telescope.nvim"
    },
    {
      "customType": "regex",
      "description": "nvim-tree/nvim-tree.lua",
      "fileMatch": ["^lua/config/plugins/nvim-tree\\.lua$"],
      "matchStrings": [
        "\"nvim-tree/nvim-tree\\.lua\"[^}]*version\\s*=\\s*\"(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "nvim-tree/nvim-tree.lua",
      "extractVersionTemplate": "^nvim-tree-v(?<version>.*)$"
    },
